.page{height:100vh;background-color:#fbfcfdFF;box-sizing:border-box;padding:32rpx}.page .empty{display:flex;justify-content:center;align-items:center;height:400rpx}.page .user-item{display:flex;justify-content:space-between;align-items:center;padding:20rpx;border-bottom:2rpx solid #E8E9EC}.page .user-item .avator{flex-shrink:0;width:67rpx;height:67rpx;display:flex;justify-content:center;align-items:center;font-size:24rpx;background-color:#4E73F7;color:#fff;border-radius:50%}.page .user-item .info{color:#2E3C67;margin:0 auto 0 20rpx}.page .user-item .info .info-top{display:flex;justify-content:flex-start;align-items:baseline;gap:20rpx;font-size:30rpx;margin-bottom:10rpx}.page .user-item .info .info-top .sub-text{font-size:24rpx}.page .user-item .info .id-num{font-size:24rpx}.page .user-item .phone{font-size:24rpx;color:#C2C2C2}.page .btn-content{position:fixed;bottom:0;left:0;right:0;display:flex;justify-content:center;align-items:center;padding:40rpx}.page .btn-content .btn{width:600rpx;background-color:#4E73F7;border-color:#4E73F7}