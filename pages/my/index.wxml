<view class="page">
    <view class="header">
        <view class="page-title">我的</view>
        <view class="header-info">
            <image class="avator" src="{{userInfo.headImage}}" />
            <view>
                <view class="phone">{{userInfo.name}}</view>
                <view class="account">账号：{{userInfo.nickname}}</view>
            </view>
            <view class="btn-self">
                <navigator url="/pages/my-info/index">个人资料</navigator>
            </view>
        </view>
    </view>
    <van-cell-group>
        <van-cell title="家庭成员" is-link icon="wap-home-o" url="/pages/my-family/index" />
        <van-cell title="我的专家" is-link icon="friends-o" url="/pages/my-doctors/index" />
        <van-cell title="在线问诊历史" is-link icon="comment-o" url="/pages/order-diagnosis/index?type=1" />
        <van-cell title="快速问诊历史" is-link icon="notes-o" url="/pages/order-diagnosis/index?type=2" />
        <van-cell title="我的服务" is-link icon="gem-o" url="/pages/my-service/index" />
        <van-cell title="我的扫码记录" is-link icon="qr" url="/pages/my-scan-record/index" />
        <van-cell title="设置" is-link icon="setting-o" url="/pages/setting/index" />
    </van-cell-group>
</view>