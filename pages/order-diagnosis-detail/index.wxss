.page{height:100vh}.page .nav-bg{position:relative;background:linear-gradient(67deg, #517CFF 0%, #809FFF 100%)}.page .nav-bg .back-icon{display:block;position:absolute;left:20rpx;font-size:48rpx}.page .nav-bg .nav-title{position:absolute;left:50%;transform:translateX(-50%);color:#fff;font-size:28rpx}.page .nav-bg .detail-top{padding:48rpx;color:#fff}.page .nav-bg .detail-top .state{font-weight:bold;font-size:36rpx;margin-bottom:16rpx}.page .nav-bg .detail-top .sub-state{font-size:26rpx}.page .detail-content{padding:20rpx 50rpx;color:#2E3C67}.page .detail-content .text{font-size:26rpx;line-height:2.5}.page .detail-content .doctor-card{margin:20rpx 0;width:100%;height:169rpx;background:#FFFFFF;box-shadow:0rpx 0rpx 20rpx 0rpx #ECEFF4;border-radius:16rpx;display:flex;justify-content:space-between;align-items:center}.page .detail-content .doctor-card .avator{width:100rpx;height:100rpx;border-radius:50%}.page .detail-content .doctor-card .doctor-info{flex:1;margin-left:20rpx}.page .detail-content .doctor-card .doctor-info .name{font-size:32rpx;font-weight:bold}.page .detail-content .doctor-card .doctor-info .name .major{margin:0 20rpx;font-weight:normal;font-size:26rpx}.page .detail-content .doctor-card .doctor-info .address{font-size:26rpx;color:#929397}.page .detail-content .doctor-card .doctor-info .address .dept{margin:0 20rpx}.page .detail-bottom{padding:0 50rpx;line-height:2.5}.page .detail-bottom .label{font-size:26rpx;color:#9D9DA1}.page .detail-bottom .label .val{font-size:26rpx;color:#2E3C67}.page .btn-content{padding:50rpx;box-sizing:border-box}.page .btn-content .btn{box-sizing:border-box;width:100%;padding:16rpx;display:flex;justify-content:center;align-items:center;background-color:#4E73F7;color:#fff;border-radius:40rpx}.page .order-code{padding:0 50rpx;font-size:26rpx;color:#9D9DA1;line-height:2.5}