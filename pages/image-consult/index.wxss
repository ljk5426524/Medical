.page{min-height:100vh;padding:20rpx 24rpx;box-sizing:border-box}.page .tip{margin:0 auto;box-sizing:border-box;color:#C88501;font-size:26rpx;padding:23rpx;display:flex;justify-content:center;align-items:center;width:630rpx;background-color:#FEF5E4}.page .name{margin:40rpx 24rpx;display:flex;justify-content:space-between;align-items:center;padding:20rpx 0;border-bottom:2rpx solid #E8E9EC}.page .name .label{font-size:30rpx;color:#2E3C67}.page .name .text{font-size:24rpx;color:#4C78FF}.page .desc{margin:40rpx 0;padding:20rpx 0;color:#2E3C67}.page .desc .label{padding-left:24rpx;font-size:36rpx;font-weight:bold}.page .pic{margin:40rpx 0;padding:20rpx 0;color:#2E3C67}.page .pic .label{padding-left:24rpx;font-size:36rpx;font-weight:bold}.page .pic .pic-content{padding:24rpx}.page .family-content{width:660rpx}.page .family-content .title{color:#fff;padding:40rpx;font-size:32rpx;background:linear-gradient(#527dff 0%, #80a0ff 100%)}.page .btn-content{margin:40rpx 0;width:100%;display:flex;justify-content:center}.page .btn-content .btn{width:630rpx;height:80rpx;display:flex;justify-content:center;align-items:center;background-color:#4C78FF;color:#fff;border-radius:50rpx}